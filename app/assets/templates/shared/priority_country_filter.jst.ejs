<div class="ui-line-item-theme">
  <div class="pin pin-small pin-white pull-right">
    <div class="pin-tip"></div>
    <div class="pin-body"></div>
  </div>
  <div class="bold">Priority Countries</div>
  <div>Filter by:</div>
</div>
<% for (var i = 0; i < strategies.length; i++) { %>
<div class="visio-check ui-line-item-theme">
