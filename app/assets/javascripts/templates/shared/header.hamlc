%section.row
  .col-md-3.menu-btn.header-item.pull-left
    %a.menu-icon
    .logo


  .col-md-6.center-text.header-item.menu-options
    - _.each _.values(context.menuOptions), (menuOption, i, list) ->
      .dashboard-item{ :data => { :option => menuOption.key } }
        %span.dashboard-label
          = menuOption.human

      - if i < list.length - 1
        %span
          = '|'

  .col-md-3.header-item
    .pull-right
      %h6.dashboard-item
        %a{ :href => "/users/sign_out", :data => { :method => "delete" }, :rel => "nofollow" }
          = Visio.user.get('login')
      %a.dashboard-item.search-icon{ :href => '#search' }

.option-menu.zero-max-height.no-border
#global-search.zero-max-height.row

